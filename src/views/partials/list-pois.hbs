{{#each pois}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{title}}
    </h2>
    <table class="table">
        <thead>
          <tr>
            <th>Description</th>
            <th>Latitude</th>
            <th>Longitude</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
            <tr>
                <td>
                  {{description}}
                </td>
                <td>
                  {{latitude}}
                </td>
                <td>
                  {{longitude}}
                </td>
                <td>
                    <a href="/poi/{{_id}}" class="button">
                        <span class="icon is-small">
                            <i class="fas fa-folder-open"></i>
                        </span>
                    </a>
                </td>
                <td>
                    <a href="/dashboard/deletepoi/{{_id}}" class="button">
                        <i class="fas fa-trash"></i>
                    </a>
                </td>
            </tr>
                        </tbody>
            </table>
                        </div>
            {{/each}}
